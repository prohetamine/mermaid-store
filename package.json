{
  "name": "mermaid-store",
  "version": "1.0.0",
  "description": "Webcam Tool",
  "author": "",
  "main": "back/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win --mac"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.2",
    "base-64": "^1.0.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "electron-npm-install": "^0.1.0",
    "express": "^4.18.2",
    "html-escaper": "^3.0.3",
    "http": "^0.0.1-security",
    "kill-port": "^2.0.1",
    "node-mermaid": "^2.2.321",
    "open": "^8.4.0",
    "path": "^0.12.7",
    "sleep-promise": "^9.1.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "electron": "^21.2.3",
    "electron-builder": "^23.6.0",
    "eslint": "^8.31.0"
  },
  "build": {
    "appId": "com.electron.prohetamine.mermaid",
    "compression": "maximum",
    "mac": {
      "category": "tool",
      "target": "dmg",
      "executableName": "mermaid-store",
      "asar": false
    },
    "dmg": {
      "icon": "icon.png",
      "background": "background.png",
      "title": "mermaid-store"
    },
    "win": {
      "target": "nsis"
    }
  }
}
